<h1><%= "#{@employee.first_name} #{@employee.last_name}" %></h1>

<img src="<%= @employee.img_url %>" >

<h3>Alias: <%= @employee.alias %></h3>
<h3>Title: <%= @employee.title  %></h3>
<h3>Office: <%= @employee.office %></h3>

<h3>Dogs:</h3>
<ul><% @employee.dogs.each do |d| %>
  <li><%= "#{d.name}"  %></li>
<% end %>
</ul>
